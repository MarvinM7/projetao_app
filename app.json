{
  "expo": {
    "name": "Projetao App",
    "slug": "projetao_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.projetao.app",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
            "certificateHash": "F5:AF:87:B6:E9:FF:3E:9A:BE:D1:23:A4:8A:5E:32:D4:FF:00:BB:E5"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}